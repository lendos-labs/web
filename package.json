{
  "name": "@lendos/web",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "turbo format",
    "format:prettier": "prettier --write .",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "lint:prettier": "prettier --check .",
    "clean": "npm exec --workspaces -c 'rm -rf node_modules .next .turbo coverage dist compiled build build-storybook' && rm -rf node_modules",
    "remove-turbo-cache": "rm -rf ./node_modules/.cache/turbo",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "@aave/math-utils": "1.25.0",
    "bignumber.js": "^9.1.2",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@lendos/configs": "workspace:*",
    "@tsconfig/next": "2.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/vite-react": "3.0.2",
    "@types/node": "^20",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "prettier": "^3.2.5",
    "turbo": "^2.5.0",
    "typescript": "5.5.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
